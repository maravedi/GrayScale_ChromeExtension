{
"update_url":"http://clients2.google.com/service/update2/crx",
  "name": "GrayScale",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Grayscale the current page.",
  "permissions": [
    "tabs", "http://*/*", "https://*/*"
  ],
  "background": {
	"scripts": ["background.js"]
  },
  "browser_action": {
	"default_icon": "off.png",
  "default_title": "icon"
  }
}
